<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FronEndProyecto.vistas.login">

    <StackLayout>

        <Label Text="Inicio de Sesion" FontSize="Title" FontAttributes="Bold"/>

        <Label Text="Correo:"/>
        <Entry x:Name="correoDos"/>

        <Label Text="contrasena:"/>
        <Entry x:Name="contraDos"/>

        <Button Text="entrar" Clicked="IniciarSesion"/>
        <Button Text="Aun no tines cuenta?" Clicked="Registrar"/>


    </StackLayout>
</ContentPage>